# General
ENV=dev
LOG_LEVEL=INFO

# OpenAI / LangSmith
OPENAI_API_KEY=
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=aoro
LANGSMITH_TRACING=true

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=neo4jpassword

# Pinecone
PINECONE_API_KEY=
PINECONE_ENVIRONMENT=
PINECONE_INDEX=aoro-case-studies

# Postgres (optional for later RLS / tenant metadata)
DATABASE_URL=postgresql+psycopg://aoro:aoro@localhost:5432/aoro

# Multi-tenancy
# Comma-separated list of tenant ids allowed in local/dev.
TENANTS=demo

# ServiceTitan
SERVICETITAN_CLIENT_ID=
SERVICETITAN_CLIENT_SECRET=
SERVICETITAN_APP_KEY=
SERVICETITAN_BASE_URL=
SERVICETITAN_TENANT_ID=
